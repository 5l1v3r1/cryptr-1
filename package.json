{
  "name": "cryptr",
  "author": "Jacob Crowther",
  "version": "1.0.7",
  "description": "A password-manager GUI built for SaltStack's GPG Renderer",
  "main": "app/main.js",
  "repository": {
    "url": "https://github.com/jcrowthe/cryptr.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "electron app/main.js",
    "build": "electron-zip-packager ./ Cryptr --platform=all --arch=all --app-version=1.0.7 prune --asar --icon=app/images/cryptr --overwrite --out=dist --ignore=dist --ignore=README.md"
  },
  "devDependencies": {
    "devtron": "^1.3.0",
    "electron-packager": "^7.7.0",
    "electron-prebuilt": "^1.3.4",
    "electron-zip-packager": "^3.0.0"
  }
}
